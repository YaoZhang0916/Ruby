<h1><%= @dojo.branch %></h1>

Address: <%= @dojo.street %>
City: <%= @dojo.city %>
State: <%= @dojo.state %>

<h2>Student Roster</h2>

<table>
	<thead>
		<tr>
			<th>First Name</th>
			<th>Last Name</th>
			<th>Email</th>
			<th>Action</th>
		</tr>
	</thead>
	
	<tbody>
		<% @student.each do |student| %>
		<tr>
			<td><%= student.first_name %></td>
			<td><%= student.last_name %></td>
			<td><%= student.email %></td>
			<td><a href="/dojos/<%= student.dojo_id %>/students/<%= student.id %>">show</a></td>
		</tr>
		<% end %>
	</tbody>	
</table>