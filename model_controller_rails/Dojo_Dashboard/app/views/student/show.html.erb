<%= @dojo.branch %>'s <%= @student.first_name%> <%= @student.last_name %>

<p><%= @student.email %></p>

<h2>Cohort</h2>

<table>
	<thead>
		<tr>
			<th>Name</th>
			<th>Email</th>
		</tr>
	</thead>
	
	<tbody>
		<% @students.each do |student| %>
		<tr>
			<td><%= student.first_name %><%= student.last_name %></td>
			<td><%= student.email %></td>
		</tr>
		<% end %>
	</tbody>	
</table>